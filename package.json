{
  "name": "cfa-own-electron",
  "productName": "CfA OWN Electron",
  "version": "1.0.0-beta.53",
  "description": "Explore NASA images",
  "main": ".webpack/main",
  "scripts": {
    "preinstall": "git config core.hooksPath .githooks",
    "start": "electron-forge start",
    "start-admin": "electron-forge start -- --admin",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "publish-dry-run": "electron-forge publish --dry-run",
    "publish-from-dry-run": "electron-forge publish --from-dry-run",
    "lint": "echo \"No linting configured\"",
    "debug": "electron-forge start  --inspect-electron",
    "debug-stop": "electron-forge start -- --inspect-brk",
    "debug-admin": "electron-forge start --inspect-electron",
    "debug-stop-admin": "electron-forge start -- --inspect-brk --admin",
    "env": "echo \"node --version: \"; node --version; echo \"electron --version: \"; electron --version; echo \"electron-forge: \"; npm list webpack",
    "help": "electron-forge --help"
  },
  "keywords": [],
  "author": {
    "name": "Stephen Bannasch",
    "email": "stephen.bannasch@gmail.com"
  },
  "license": "UNLICENSED",
  "private": false,
  "config": {
    "forge": {
      "packagerConfig": {},
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "stepheneb",
              "name": "cfa-own-electron"
            },
            "prerelease": true,
            "draft": true
          }
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cfa_owns_electron"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "background": "src/images/background.jpg",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "default-src * 'self' 'unsafe-eval' 'unsafe-inline' data:; script-src 'self' 'unsafe-eval' 'unsafe-inline' data:",
            "devServer": {},
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                },
                {
                  "html": "./src/renderer/admin.html",
                  "js": "./src/renderer/admin_renderer.js",
                  "name": "admin_window",
                  "preload": {
                    "js": "./src/renderer/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@popperjs/core": "^2.9.3",
    "axios": "^0.25.0",
    "axios-rate-limit": "^1.3.0",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.6.0",
    "electron-settings": "^4.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "lodash": "^4.17.21",
    "lowdb": "^3.0.0",
    "platform": "^1.3.6",
    "simple-keyboard": "^3.3.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-dmg": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.61",
    "@electron-forge/publisher-github": "^6.0.0-beta.61",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "autoprefixer": "^10.3.3",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.2.0",
    "electron": "13.2.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.2.1",
    "node-loader": "^2.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "resolve-url-loader": "^5.0.0-beta.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "url-loader": "^4.1.1",
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "babel-jest": "^27.1.0",
    "browser-sync": "^2.27.5",
    "esm": "^3.2.25",
    "gulp": "^4.0.2"
  }
}
